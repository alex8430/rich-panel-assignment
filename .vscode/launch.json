{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "attach",
          "name": "Docker: Attach to Node",
          "port": 9229,
          "address": "localhost",
          "localRoot": "${workspaceFolder}/build",
          "remoteRoot": "/usr/src/app/build",
          "protocol": "inspector",
          "restart": true,
          "skipFiles": ["<node_internals>/**"]
      }
  ]
}